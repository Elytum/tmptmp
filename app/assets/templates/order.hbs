<div class="order">
  <div class="row">
    <div class="form-group">
      <div class="col-xs-9">
        <input type="text" class="form-control" placeholder="Départ...">
      </div>
      <div class="col-xs-3">
      <input id="start-place" class="btn btn-primary" type="button" value="Choisir">
      </div>
    </div>
  </div><!-- /.row -->
  <div class="row">
    <div class="form-group">
      <div class="col-xs-9">
        <input type="text" class="form-control" placeholder="Arrivée...">
      </div>
      <div class="col-xs-3">
        <input class="btn btn-primary" type="button" value="Choisir">
      </div>
    </div>
  </div><!-- /.row -->
  <div class="row">
    <div class="form-group">
      <div class="col-xs-6">
        <input type="date" class="form-control" name="startDate">
      </div>
      <div class="col-xs-6">
        <input type="time" class="form-control" name="startTime">
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12 text-center">
      <div class="form-group">
        <input type="number" name="quantity" min="1" max="5" value="0">
        <i class="fa fa-suitcase fa"></i>&nbsp;
        <input type="number" name="quantity" min="1" max="5" value="0">
        <i class="fa fa-suitcase fa-2x"></i>&nbsp;
        <input type="number" name="quantity" min="1" max="5" value="0">
        <i class="fa fa-suitcase fa-3x"></i>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12 text-center">
      <div class="form-group">
        <input class="btn btn-primary btn-lg" type="submit" value="Réserver">
      </div>
    </div>
  </div>
  <div id="map" style="display: none; width: 500px;height: 400px;"></div>
</div>
